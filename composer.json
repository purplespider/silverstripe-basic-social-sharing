    "type": "silverstripe-vendormodule",
    "homepage": "https://github.com/purplespider/silverstripe-basic-social-sharing",
    "keywords": ["silverstripe", "social media", "meta tags"],
    "license": "BSD-3-Clause",
    "authors": [{
        "name": "James Cocker",
        "email": "ssmodules@pswd.biz"
    }],
    "support": {
        "issues": "https://github.com/purplespider/silverstripe-basic-social-sharing/issues"
    },
    "require": {
        "php": ">=8.3",
        "silverstripe/cms": "^6.0"
    },
    "autoload": {
        "psr-4": {
